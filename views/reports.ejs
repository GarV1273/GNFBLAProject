<!DOCTYPE html>
<html lang="en">

<%- include('./partials/head'); %>

  <body>

    <%- include('./partials/header'); %>

      <%- include('./partials/sidebar'); %>

        <main id="main" class="main">

          <div class="pagetitle">
            <h1>Reports</h1>
            <nav>
              <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="index.html">Home</a></li>
                <li class="breadcrumb-item active">Reports</li>
              </ol>
            </nav>
          </div><!-- End Page Title -->

          <div class="row">
            <div class="col-lg-3 col-md-6 col-sm-12">
              <div class="row">
                <h3 class="text-center">9th Grade</h3>
              </div>
              <table class="table">
                <thead>
                  <tr>
                    <th scope="col">Student Name</th>
                    <th scope="col">Points</th>
                  </tr>
                </thead>
                <tbody>
                  <% for (let i = 0; i < gradeNine.length; i++) { %>
                    <tr>
                      <th scope="row"><%= gradeNine[i].name %></th>
                      <td><%= gradeNine[i].points %></td>
                    </tr>
                  <% } %>
                </tbody>
              </table>
            </div>
            <div class="col-lg-3 col-md-6 col-sm-12">
              <div class="row">
                <h3 class="text-center">10th Grade</h3>
              </div>
              <table class="table">
                <thead>
                  <tr>
                    <th scope="col">Student Name</th>
                    <th scope="col">Points</th>
                  </tr>
                </thead>
                <tbody>
                  <% for (let i = 0; i < gradeTen.length; i++) { %>
                    <tr>
                      <th scope="row"><%= gradeTen[i].name %></th>
                      <td><%= gradeTen[i].points %></td>
