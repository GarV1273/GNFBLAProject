                  <% } %>
                </tbody>
              </table>
            </div>
            <div class="col-lg-3 col-md-6 col-sm-12">
              <div class="row">
                <h3 class="text-center">11th Grade</h3>
              </div>
              <table class="table">
                <thead>
                  <tr>
                    <th scope="col">Student Name</th>
                    <th scope="col">Points</th>
                  </tr>
                </thead>
                <tbody>
                  <% for (let i = 0; i < gradeEleven.length; i++) { %>
