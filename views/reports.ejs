                      <th scope="row"><%= gradeEleven[i].name %></th>
                      <td><%= gradeEleven[i].points %></td>
                    </tr>
                  <% } %>
                </tbody>
              </table>
            </div>
            <div class="col-lg-3 col-md-6 col-sm-12">
